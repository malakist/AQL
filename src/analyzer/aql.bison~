%{
	#include <stdlib.h>
	#include <stdio.h>

	#define YYSTYPE char const *
	
	int yylex(void);
	void yyerror(const char *);
%}

%token IDENTIFIER

%%

column_id:
			IDENTIFIER					{ printf("Identificador encontrado: %s\n", $1); }
			;
				
%%

int yywrap(void) {
	return 1;
}

int main(void) {
	yyparse();
	return 0;
}

void yyerror(const char *s) {
	fprintf(stderr, "%s\n", s);
}